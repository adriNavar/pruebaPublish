                            <div class="@tamanioA centrado h4">@textocomp</div>
                        }
                        @if (Model.TierraPorStore.TipoParametro2 == "combo" || Model.TierraPorStore.TipoParametro2 == "texto" || Model.TierraPorStore.TipoParametro2 == "comparador")
