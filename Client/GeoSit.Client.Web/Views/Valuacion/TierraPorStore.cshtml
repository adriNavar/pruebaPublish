@model GeoSit.Client.Web.Models.ValuacionModel
@{
    ViewBag.Title = "Valores Básicos de la Tierra por Store";
    ViewBag.Description = "";
    ViewBag.MensajeEntrada = "";

}


    <link href="~/Content/valuacion.css" rel="stylesheet" />
    <link href="~/Content/jquery.jscrollpane.css" rel="stylesheet" />
    <link href="~/Content/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/formValidation.min.css" rel="stylesheet" />
    <link href="~/Content/select2.css" rel="stylesheet" />
<link href="~/Content/select2-bootstrap.css" rel="stylesheet" />


@*Modal Contenido*@
<div class="modal fade" id="myModal" tabindex="-100" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header centrado" style="padding: 10px 0px 40px;">
                <h4 class="modal-title col-lg-12 col-xs-12 col-sm-12 col-md-12" id="myModalLabel">@ViewBag.Title</h4>
            </div>
            <div class="modal-body   modal-body-scroll">
                @using (Ajax.BeginForm("GrabaTierraPorStore", "Valuacion", new { ReturnUrl = ViewBag.ReturnUrl }, new AjaxOptions { }, new { id = "form", role = "form", novalidate = "" }))
                {
                    @Html.AntiForgeryToken()
                    ViewBag.MensajeEntrada = Model.Mensaje;
                    //@Html.Hidden("model.Parcela.Tipo_Parcela", Model.Parcela.Tipo_Parcela)
                    @Html.Hidden("model.IdFiltroParcela", Model.IdFiltroParcela)
                    @Html.Hidden("model.TipoValuacionId", Model.TipoValuacionId)
                    var textocomp = "Comparación";
                    var tamanioA = "col-lg-3 col-xs-3 col-sm-3 col-md-3";
                    var tamanioB = "col-lg-3 col-xs-3 col-sm-3 col-md-3";
                    if (Model.TierraPorStore.TipoParametro1 == "comparador" && Model.TierraPorStore.TipoParametro2 == "comparador")
                    {
                        tamanioA = "col-lg-2 col-xs-2 col-sm-2 col-md-2";
                        textocomp = "Comp.";
                    }
                    else if (Model.TierraPorStore.TipoParametro1 == "comparador" && (Model.TierraPorStore.TipoParametro2 != "combo" && Model.TierraPorStore.TipoParametro2 != "texto" && Model.TierraPorStore.TipoParametro2 != "comparador") ||
                         Model.TierraPorStore.TipoParametro2 == "comparador" && (Model.TierraPorStore.TipoParametro1 != "combo" && Model.TierraPorStore.TipoParametro1 != "texto" && Model.TierraPorStore.TipoParametro1 != "comparador"))
                    {
                        tamanioA = "col-lg-4 col-xs-4 col-sm-4 col-md-4";
                        tamanioB = "col-lg-4 col-xs-4 col-sm-4 col-md-4";
                    }
                    else if ((Model.TierraPorStore.TipoParametro1 != "combo" && Model.TierraPorStore.TipoParametro1 != "texto" && Model.TierraPorStore.TipoParametro1 != "comparador" && Model.TierraPorStore.TipoParametro1 != "comparador") &&
                         (Model.TierraPorStore.TipoParametro2 != "combo" && Model.TierraPorStore.TipoParametro2 != "texto" && Model.TierraPorStore.TipoParametro2 != "comparador" && Model.TierraPorStore.TipoParametro2 != "comparador"))
                    {
                        tamanioA = "col-lg-12 col-xs-12 col-sm-12 col-md-12";
                        tamanioB = "col-lg-12 col-xs-12 col-sm-12 col-md-12";
                    }                   
                    else if ((Model.TierraPorStore.TipoParametro1 != "combo" && Model.TierraPorStore.TipoParametro1 != "texto" && Model.TierraPorStore.TipoParametro1 != "comparador" && Model.TierraPorStore.TipoParametro1 != "comparador" ) ||
                        (Model.TierraPorStore.TipoParametro2 != "combo" && Model.TierraPorStore.TipoParametro2 != "texto" && Model.TierraPorStore.TipoParametro2 != "comparador" && Model.TierraPorStore.TipoParametro2 != "comparador") )
                    {
                        tamanioA = "col-lg-6 col-xs-6 col-sm-6 col-md-6";
                        tamanioB = "col-lg-6 col-xs-6 col-sm-6 col-md-6";
                    }
                    else if ((Model.TierraPorStore.TipoParametro1 != "comparador" && Model.TierraPorStore.TipoParametro2 != "comparador") ||
                        (Model.TierraPorStore.TipoParametro1 != "combo" && Model.TierraPorStore.TipoParametro1 != "texto" && Model.TierraPorStore.TipoParametro1 != "comparador") ||
                        (Model.TierraPorStore.TipoParametro2 != "combo" && Model.TierraPorStore.TipoParametro2 != "texto" && Model.TierraPorStore.TipoParametro2 != "comparador"))
                    {
                        
                        tamanioA = "col-lg-4 col-xs-4 col-sm-4 col-md-4";
                        tamanioB = "col-lg-4 col-xs-4 col-sm-4 col-md-4";
                    }
                   
                    
