                                {
                                    var comparador = long.Parse(Model.TierraPorStore.ValorComparacion2[i]);
                                    margensegundo = "";
                                    if (i > 0)
                                    {
                                        margensegundo = "margin-top: 10px;";
                                    }
                                    <div class="container-fluid sin-padding">

                                        <div class="col-lg-12 col-xs-12 col-sm-12 col-md-12 sin-padding" tabindex="1" style="@margensegundo">
                                            <select class="form-control sin-padding divOpciones cmbcomparacion2" id="Comparacion2_@i" name="model.TierraPorStore.ValorComparacion2">
                                                @if (comparador == 1)
