                            <div class="@tamanioA centrado h4">@textocomp</div>
                        }
                        @if (Model.TierraPorStore.TipoParametro1 == "combo" || Model.TierraPorStore.TipoParametro1 == "texto" || Model.TierraPorStore.TipoParametro1 == "comparador")
                        {
                            <div class="@tamanioB centrado h4" style="white-space:nowrap">@ViewBag.NombreParametro1</div>
                        }
                        @if (Model.TierraPorStore.TipoParametro2 == "comparador")
